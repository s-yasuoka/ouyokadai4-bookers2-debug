<% if users.exists? %>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
  <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(50,50) %></td>
        <td><%= user.name %></td>
        <td>フォロー:<%= user.followings.count %></td>
        <td>フォロワー:<%= user.followers.count %></td>
        <td><%= link_to "show",user_path(user.id) %></td>
  <% end %>
      </tr>
    </tbody>
  </table>

<% else %>
  <p>no user</p>
<% end %>